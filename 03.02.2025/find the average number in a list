def find_average(numbers):
    if not numbers: 
        return None 
    total = sum(numbers)  
    average = total / len(numbers) 
    return average
number_list = [10, 20, 30, 40, 50]
average = find_average(number_list)
if average is not None:
    print("Average of numbers in the list:", average)
else:
    print("The list is empty.")
